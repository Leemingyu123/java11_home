package org.kh.java;

public class Operation6 {

	public static void main(String[] args) {
		int a = 26;
		int b = 28;
		int c  = 27;

		system.out.println(”a >b && a >c : “+(a > b && a > c )); 
		system.out.println(”a >b && b >c : “+(a > b && b > c )); 
		system.out.println(”a <b && b <c : “+(a < b && b < c ));
		
	}

}
