package org.kh.java;

public class asfdsf {

	public static void main(String[] args) {
		char ch6 = '\u26BD';
		System.out.println("ch6 :  "+ch6);
		System.out.println("16진수 : "+Integer.toHexString(ch6));
		System.out.println("10진수 : "+Integer.toBinaryString(ch6));
		System.out.println("10진수 : "+Integer.toOctalString(ch6));

	}

}
